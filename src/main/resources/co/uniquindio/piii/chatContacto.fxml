<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="co.uniquindio.piii.controller.ChatContactoController"
      spacing="10" alignment="CENTER">
    <ListView fx:id="listViewUsuarios" prefHeight="200.0" prefWidth="300.0" />
    <Button text="Iniciar contacto" onAction="#handleIniciarContacto" />
    <Button text="Volver" onAction="#handleVolver" />
</VBox>
