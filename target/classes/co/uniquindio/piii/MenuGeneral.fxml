<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="15" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.uniquindio.piii.controller.MenuGeneralController">
    
    <Button fx:id="btnChatContacto" text="Chat de Contacto" onAction="#handleChatContacto"/>
    <Button fx:id="btnComentario" text="Comentario" onAction="#handleComentario"/>
    <Button fx:id="btnProducto" text="Producto" onAction="#handleProducto"/>
    <Button fx:id="btnVendedor" text="Vendedor" onAction="#handleVendedor"/>

    <VBox.margin>
        <Insets top="20" />
    </VBox.margin>
</VBox>
